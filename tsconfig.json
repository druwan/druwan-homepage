{
  "compilerOptions": {
    "target": "ES6",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/index.js"],
  "exclude": ["node_modules"]
}
