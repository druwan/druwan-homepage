{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ES6",
    "moduleResolution": "Node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strict": true,
    "target": "ES6"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/index.js"],
  "exclude": ["node_modules"]
}
